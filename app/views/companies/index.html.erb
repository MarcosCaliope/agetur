<p id="notice"><%= notice %></p>

<h1>Empresa</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cnpj</th>
      <th>Address</th>
      <th>Phone</th>
      <th>City</th>
      <th>State</th>
      <th>Osmodel</th>
      <th>Osdupla</th>
      <th>Email</th>
      <th>Site</th>
      <th>Logoform</th>
      <th>Logoentrada</th>
      <th>Iretorno</th>
      <th>Osshowcan</th>
      <th>Osshowcanrel</th>
      <th>Osshowrep</th>
      <th>Osincludechdcalc</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.cnpj %></td>
        <td><%= company.address %></td>
        <td><%= company.phone %></td>
        <td><%= company.city %></td>
        <td><%= company.state.uf %></td>
        <td><%= company.osmodel %></td>
        <td><%= company.osdupla %></td>
        <td><%= company.email %></td>
        <td><%= company.site %></td>
        <td><%= image_tag company.logoform, midth:25, height:25%></td>
        <td><%= image_tag company.logoentrada, midth:25, height:25%></td>

        <td><%= company.iretorno %></td>
        <td><%= company.osshowcan %></td>
        <td><%= company.osshowcanrel %></td>
        <td><%= company.osshowrep %></td>
        <td><%= company.osincludechdcalc %></td>
        <td><%= company.comments %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
