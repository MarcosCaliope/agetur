<p id="notice"><%= notice %></p>

<h1>Vendors</h1>

<table>
  <thead>
    <tr>
      <th>Sname</th>
      <th>Email</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Comments</th>
      <th>Commission</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendors.each do |vendor| %>
      <tr>
        <td><%= vendor.sname %></td>
        <td><%= vendor.email %></td>
        <td><%= vendor.address %></td>
        <td><%= vendor.phone %></td>
        <td><%= vendor.comments %></td>
        <td><%= vendor.commission %></td>
        <td><%= link_to 'Show', vendor %></td>
        <td><%= link_to 'Edit', edit_vendor_path(vendor) %></td>
        <td><%= link_to 'Destroy', vendor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor', new_vendor_path %>
